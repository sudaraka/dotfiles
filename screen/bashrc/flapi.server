# If not running interactively, don't do anything
[[ $- != *i* ]] && return

. ~/.bashrc

pyve flapi

supervisord -c etc/supervisord-dev.conf
trap 'supervisorctl -c etc/supervisord-dev.conf shutdown' EXIT SIGTERM SIGKILL

supervisorctl -c etc/supervisord-dev.conf tail -f http_flask
