# If not running interactively, don't do anything
[[ $- != *i* ]] && return

. ~/.bashrc

alias tree='tree -C --dirsfirst -I "__pyc*|node_modules*|coverage*"'

pyve msc-calc

# Open code editor
gvim

g s
